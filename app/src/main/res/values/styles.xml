<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Btn" parent="Widget.Material3.Button">
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:backgroundTint">@color/DARKBLUE04</item>
    </style>
    <style name="code">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#151515</item>
    </style>
    <style name="Widget.App.Switch" parent="Widget.Material3.CompoundButton.Switch">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Switch</item>
    </style>

    <style name="ThemeOverlay.App.Switch" parent="">
        <item name="colorPrimaryContainer">@color/DARKBLUE05</item>
        <item name="colorPrimary">@color/white</item>
    </style>
</resources>